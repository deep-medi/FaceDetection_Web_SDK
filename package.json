{
  "name": "@deep-medi/face-detection-web-sdk",
  "version": "0.1.0",
  "description": "웹 기반 얼굴 인식을 통해 실시간으로 심박수, 스트레스, 혈압 등의 건강 정보를 측정하는 SDK",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "examples",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.js"
    }
  },
  "keywords": [
    "face-detection",
    "mediapipe",
    "health-monitoring",
    "biometric",
    "web-sdk",
    "typescript",
    "ppg",
    "heart-rate"
  ],
  "author": "KO Jong Won <gojw@deep-medi.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deep-medi/FaceDetection_Web_SDK.git"
  },
  "bugs": {
    "url": "https://github.com/deep-medi/FaceDetection_Web_SDK/issues"
  },
  "homepage": "https://github.com/deep-medi/FaceDetection_Web_SDK#readme",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:lib": "vite build --mode lib",
    "lint": "eslint .",
    "lint:fix": "npx eslint . --fix",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build:lib",
    "test": "echo \"Error: no test specified\" && exit 1",
    "demo": "vite examples/basic-demo --port 8080 --host 0.0.0.0"
  },
  "peerDependencies": {
    "@mediapipe/face_detection": "^0.4.1646425229"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/node": "^22.15.23",
    "@vitejs/plugin-basic-ssl": "^2.0.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "terser": "^5.40.0",
    "typescript": "^5.8.3",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.4"
  },
  "dependencies": {
    "path": "^0.12.7"
  }
}
